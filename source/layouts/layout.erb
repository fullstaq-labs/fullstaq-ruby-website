<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Fullstaq Ruby" %></title>
    <meta name="description" content="A new Ruby distribution for production environments. Less memory, faster, more secure, fully open source, based on MRI.">
    <%= stylesheet_link_tag 'elate/bootstrap.css', 'elate/main.css', 'custom.css', 'icomoon.css' %>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,600,400italic,700" rel="stylesheet" type="text/css">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@fullstaq">
    <meta name="twitter:creator" content="@honglilai">
    <meta name="twitter:title" content="Fullstaq Ruby: Ruby, optimized for production">
    <meta name="twitter:description" content="A new Ruby distribution for production environments. Less memory, faster, more secure, fully open source, based on MRI.">
    <meta name="twitter:image" content="<%= config[:root_url] %>/<%= image_path 'ruby-blue-opengraph.jpg' %>">

    <meta property="og:title" content="Fullstaq Ruby: Ruby, optimized for production">
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%= config[:root_url] %>/">
    <meta property="og:description" content="New Ruby distro for production environments. Less memory, faster, more secure, fully open source, based on MRI.">
    <meta property="og:image" content="<%= config[:root_url] %>/<%= image_path 'ruby-blue-opengraph.jpg' %>">
    <meta property="og:image:width" content="822">
    <meta property="og:image:height" content="752">
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-landing-page">
      <div class="container">
        <% link_to '/index.html', class: 'navbar-brand navbar-brand-fullstaq-ruby' do %><img src="<%= image_path 'fullstaq-logo-small.png' %>" srcset="<%= image_path 'fullstaq-logo-small.png' %> 1x, <%= image_path 'fullstaq-logo-small@2x.png' %> 2x" class="logo" alt="" aria-hidden="true"><span class="vendor">Fullstaq</span> <span class="product">Ruby</span> <sup class="beta-disclaimer">beta</sup><% end %>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div id="navbarSupportedContent" class="collapse navbar-collapse">
          <ul class="navbar-nav ml-auto">
            <!--
            <% navbar_dropdown 'Get Started', :get_started, @current_section do %>
              <%= navbar_dropdown_item 'For Servers', [:get_started, :get_started_for_servers], @current_section, '#' %>
              <div class="dropdown-divider"></div>
              <a href="#" class="dropdown-item disabled">For Containers <span class="badge">coming soon!</span></a>
              <a href="#" class="dropdown-item disabled">For Heroku <span class="badge">coming soon!</span></a>
            <% end %>
            -->

            <%= navbar_link 'Get Started', :get_started, @current_section, 'https://github.com/fullstaq-labs/fullstaq-ruby-server-edition/blob/master/README.md#installation' %>
            <% navbar_dropdown 'Editions', :editions, @current_section do %>
              <%= navbar_dropdown_item %Q{Server Edition <span class="label label-primary">beta</span>}, [:editions, :server_edition], @current_section, 'https://github.com/fullstaq-labs/fullstaq-ruby-server-edition/blob/master/README.md' %>
              <div class="dropdown-divider"></div>
              <a href="#" class="dropdown-item disabled">Container Edition <span class="badge badge-secondary">coming soon!</span></a>
              <a href="#" class="dropdown-item disabled">Heroku Edition <span class="badge badge-secondary">coming soon!</span></a>
            <% end %>
            <%= navbar_link 'Docs', :docs, @current_section, 'https://github.com/fullstaq-labs/fullstaq-ruby-server-edition/blob/master/README.md' %>
            <%= navbar_link 'Roadmap', :roadmap, @current_section, 'https://github.com/fullstaq-labs/fullstaq-ruby-umbrella/projects' %>
            <%= navbar_link 'GitHub', :github, @current_section, 'https://github.com/fullstaq-labs/fullstaq-ruby-umbrella' %>
            <%= navbar_link 'Follow', :follow, @current_section, 'https://twitter.com/honglilai' %>
            <%= navbar_link 'About', :about, @current_section, '/index.html', anchor: 'about' %>
          </ul>
        </div>
      </div>
    </nav>

    <%= yield %>

    <footer>
      <div class="container">
        <div class="brand">
          <% link_to '/index.html' do %><img src="<%= image_path 'fullstaq-logo-small.png' %>" srcset="<%= image_path 'fullstaq-logo-small.png' %> 1x, <%= image_path 'fullstaq-logo-small@2x.png' %> 2x" class="logo" alt="" aria-hidden="true"><span class="vendor">Fullstaq</span> <span class="product">Ruby</span> <sup class="beta-disclaimer">beta</sup><% end %>
        </div>

        <div>
          <nav>
            <ul>
              <li><%= link_to 'Get Started', 'https://github.com/fullstaq-labs/fullstaq-ruby-server-edition/blob/master/README.md#installation' %></li>
              <li><%= link_to 'Docs', 'https://github.com/fullstaq-labs/fullstaq-ruby-server-edition/blob/master/README.md' %></li>
              <li><%= link_to 'Roadmap', 'https://github.com/fullstaq-labs/fullstaq-ruby-umbrella/projects' %></li>
              <li><%= link_to 'GitHub', 'https://github.com/fullstaq-labs/fullstaq-ruby-umbrella' %>
              <li><%= link_to 'Follow', 'https://twitter.com/honglilai' %>
              <li><%= link_to 'About', '/index.html', anchor: 'about' %>
            </ul>
          </nav>
        </div>

        <div class="acknowledgements">
          <a href="https://www.vexels.com/vectors/preview/151224/slot-diamond-icon">Blue slot diamond icon originally designed by Vexels.com</a>
        </div>
      </div>
    </footer>

    <% if config[:google_analytics] %>
      <!-- Global site tag (gtag.js) - Google Analytics -->
      <script async src="https://www.googletagmanager.com/gtag/js?id=UA-142401740-1"></script>
      <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-142401740-1');
      </script>
    <% end %>

    <%= javascript_include_tag 'bootstrap' %>
  </body>
</html>
